    var aRed = document.createElement("a");
    aRed.href = "javascript: switchColor('red');";
    aRed.innerHTML = "Red";
    setRed.appendChild(aRed);

    var setBlack = document.createElement("li");
    setBlack.class = "left";
    var aBlack = document.createElement("a");
