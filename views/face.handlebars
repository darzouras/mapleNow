<script>
var color = "red";

function reload() {
    {{#each ariantF}}
    document.getElementById("ariant_{{image}}").src = "images/faces/" + color + "/" + "{{image}}.png";
    {{/each}}
    {{#each henesysF}}
    document.getElementById("hen_{{image}}").src = "images/faces/" + color + "/" + "{{image}}.png";
    {{/each}}
    {{#each nlcF}}
    document.getElementById("nlc_{{image}}").src = "images/faces/" + color + "/" + "{{image}}.png";
    {{/each}}
    {{#each orbisF}}
    document.getElementById("orbis_{{image}}").src = "images/faces/" + color + "/" + "{{image}}.png";
    {{/each}}
    {{#each ludiF}}
    document.getElementById("ludi_{{image}}").src = "images/faces/" + color + "/" + "{{image}}.png";
    {{/each}}
}

function switchColor(newColor) {
    if (color != newColor) {
        color = newColor;
        reload();
    }
}

window.onload = function() {
    reload();
}
</script>
