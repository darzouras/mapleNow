    {{/each}}
    {{#each showaF}}
    document.getElementById("showa_{{image}}").src = "images/faces/" + color + "/" + "{{image}}.png";
    {{/each}}
    {{#each cbdF}}
    document.getElementById("cbd_{{image}}").src = "images/faces/" + color + "/" + "{{image}}.png";
    {{/each}}
}

function switchColor(newColor) {
    if (color != newColor) {
        color = newColor;
        reload();
    }
}

window.onload = function() {
    reload();

    var footer = document.getElementById("footernav");
    document.getElementById("tweet").remove();
    document.getElementById("github").remove();
    document.getElementById("footerBrand").textContent = "Eye Colors";

    var setWhite = document.createElement("li");
    setWhite.class = "left";
    var aWhite = document.createElement("a");
    aWhite.href = "javascript: switchColor('white');";
    aWhite.innerHTML = "White";
    setWhite.appendChild(aWhite);

    var setAm = document.createElement("li");
    setAm.class = "left";
    var aAm = document.createElement("a");
    aAm.href = "javascript: switchColor('amethyst');";
    aAm.innerHTML = "Amethyst";
    setAm.appendChild(aAm);

    var setViolet = document.createElement("li");
    setViolet.class = "left";
    var aViolet = document.createElement("a");
    aViolet.href = "javascript: switchColor('violet');";
    aViolet.innerHTML = "Violet";
    setViolet.appendChild(aViolet);

    var setSapphire = document.createElement("li");
    setSapphire.class = "left";
    var aSapphire = document.createElement("a");
    aSapphire.href = "javascript: switchColor('sapphire');";
    aSapphire.innerHTML = "Sapphire";
    setSapphire.appendChild(aSapphire);

    var setHazel = document.createElement("li");
