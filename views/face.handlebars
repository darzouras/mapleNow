    aBlack.innerHTML = "Black";
    setBlack.appendChild(aBlack);

    footer.appendChild(setBlack);
    footer.appendChild(setRed);
    footer.appendChild(setBlue);
    footer.appendChild(setGreen);
    footer.appendChild(setHazel);
    footer.appendChild(setSapphire);
    footer.appendChild(setViolet);
    footer.appendChild(setAm);
